<!DOCTYPE html>
<html>
  <head>
    <title>Bookers3App</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
   
  </head>

  <body>
    <header>
      <nav class="navbar navbar-expand-lg navbar-bark bg-dark">
        <div class="container">
          <a class="navbar-brand" href="/">
            <apan>Bookers</apan>
          </a>
      
        <button class="navbar-toggler" type="button" date-taggler="collapse" date-target="#navbarnavDropdown" aria-controls="false" aria-label="Toggle navgation">
          <span class="navbar-toggler-icon"></span>
        </button>
        
        <div class="collapse navbar-collapse" id="navbarnavDropdown" style >
           <ul class="navbar-nav ml-auto">
      
      
    <% if user_signed_in? %>
      <li>
          <%= link_to 'Home', user_path(current_user.id) %>
      </li>
      <li>
          <%= link_to 'User', users_path %>
      </li>
      <li>
          <%= link_to 'Books', books_path %>
      </li>
      <li>
          <%= link_to 'Log out', destroy_user_session_path, method: :delete %>
      </li>
    <% else %>
      <li>
          <%= link_to 'Home', root_path %>
      </li>
      <li>
          <%= link_to 'About', about_path %>
      </li>
    
      <li>
           <%= link_to "Sign up", new_user_registration_path %>
      </li>
      <li>
           <%= link_to "Log in", new_user_session_path %>
      </li>
    <% end %>
      </ul>
      </div>
      </div>
    </nav>
    </header>
    <%= yield %>
  </body>
</html>